@echo off
pushd "%~dp0"
if exist %systemroot%\sysnative\cmd.exe set runsix="%SYSTEMROOT%\sysnative\cmd.exe" /C 
if not exist %windir%\system32\osk.exe goto b
goto c
:b
7z.exe x -o"%TEMP%\HBCD" -y Files\Tools.7z osk.exe
REG ADD HKCU\Software\Microsoft\Osk /v Setting /t REG_BINARY /d  /f
REG ADD HKCU\Software\Microsoft\Osk /v Stepping /t REG_DWORD /d 3 /f
CD /D "%TEMP%\HBCD" 
:c
start "" %runsix% osk.exe